NODE_ENV=production
PORT=5000

# Database (will be provided by Railway)
DB_HOST=${RAILWAY_MYSQL_HOST}
DB_PORT=${RAILWAY_MYSQL_PORT}
DB_NAME=${RAILWAY_MYSQL_DATABASE}
DB_USER=${RAILWAY_MYSQL_USER}
DB_PASSWORD=${RAILWAY_MYSQL_PASSWORD}

# JWT
JWT_SECRET=change_this_to_a_very_long_random_string
JWT_EXPIRE=7d

# Frontend URL
FRONTEND_URL=https://your-app-name.railway.app

# Logging
LOG_LEVEL=info